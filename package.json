{
  "name": "turborepo-next-boilerplate",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "format:staged": "lint-staged",
    "format:write": "prettier --write \"**/*.{js,jsx,ts,tsx,md}\"",
    "prepare": "husky install"
  },
  "devDependencies": {
    "eslint-config-custom": "*",
    "turbo": "latest",
    "husky": "^8.0.0"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "dependencies": {
    "lint-staged": "^13.1.0",
    "prettier": "^2.8.3"
  },
  "packageManager": "yarn@1.22.19"
}
